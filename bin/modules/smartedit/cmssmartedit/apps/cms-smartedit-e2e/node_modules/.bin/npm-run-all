#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/npm-run-all/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/node_modules:/Users/sandeepkalpane/Data/hybris2105/node_modules:/Users/sandeepkalpane/Data/node_modules:/Users/sandeepkalpane/node_modules:/Users/node_modules:/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-smartedit-e2e/node_modules/npm-run-all/bin/npm-run-all/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-smartedit-e2e/node_modules/npm-run-all/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-smartedit-e2e/node_modules/npm-run-all/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-smartedit-e2e/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/cmssmartedit/apps/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/cmssmartedit/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm-run-all/bin/npm-run-all/index.js" "$@"
else
  exec node  "$basedir/../npm-run-all/bin/npm-run-all/index.js" "$@"
fi
