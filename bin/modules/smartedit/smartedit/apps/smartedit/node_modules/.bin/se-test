#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/node_modules:/Users/sandeepkalpane/Data/hybris2105/node_modules:/Users/sandeepkalpane/Data/node_modules:/Users/sandeepkalpane/node_modules:/Users/node_modules:/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/smartedit/node_modules/smartedit-build/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/smartedit/node_modules/smartedit-build/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/smartedit/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --inspect "$basedir/../smartedit-build/bin/index.js" "$@"
else
  exec node  --inspect "$basedir/../smartedit-build/bin/index.js" "$@"
fi
