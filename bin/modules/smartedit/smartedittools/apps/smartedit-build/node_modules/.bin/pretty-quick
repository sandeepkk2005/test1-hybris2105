#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/pretty-quick@2.0.1_prettier@2.1.1/node_modules/pretty-quick/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/pretty-quick@2.0.1_prettier@2.1.1/node_modules/pretty-quick/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/pretty-quick@2.0.1_prettier@2.1.1/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/node_modules:/Users/sandeepkalpane/Data/hybris2105/node_modules:/Users/sandeepkalpane/Data/node_modules:/Users/sandeepkalpane/node_modules:/Users/node_modules:/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/node_modules/pretty-quick/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/node_modules/pretty-quick/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/apps/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pretty-quick/bin/pretty-quick.js" "$@"
else
  exec node  "$basedir/../pretty-quick/bin/pretty-quick.js" "$@"
fi
