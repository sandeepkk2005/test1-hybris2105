#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/typescript@3.9.3/node_modules/typescript/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/typescript@3.9.3/node_modules/typescript/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/typescript@3.9.3/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/common/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedittools/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/node_modules:/Users/sandeepkalpane/Data/hybris2105/node_modules:/Users/sandeepkalpane/Data/node_modules:/Users/sandeepkalpane/node_modules:/Users/node_modules:/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/node_modules/typescript/bin/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/node_modules/typescript/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/apps/node_modules:/Users/sandeepkalpane/Data/hybris2105/hybris/bin/modules/smartedit/smartedit/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
