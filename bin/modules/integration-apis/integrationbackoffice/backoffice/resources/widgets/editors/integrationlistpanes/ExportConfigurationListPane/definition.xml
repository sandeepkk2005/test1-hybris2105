<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="de.hybris.platform.integrationbackoffice.widgets.ExportConfigurationListPane"
                   extends="de.hybris.platform.integrationbackoffice.widgets.AbstractIntegrationListPane">
    <name>Export Configuration ListPane</name>
    <description>Allows the selection of various entity types for later mapping and exporting.</description>
    <defaultTitle>Exporting Configuration</defaultTitle>
    <author>Hybris</author>
    <version>1.0</version>
    <keywords>
        <keyword>ExportConfigurationListPane</keyword>
    </keywords>
    <controller
            class="de.hybris.platform.integrationbackoffice.widgets.configuration.controllers.ExportConfigurationListPaneController"/>
    <sockets>
        <input id="clearListPane"
               type="java.lang.String"/>
        <input id="currentSelectedCount" type="java.lang.String"/>
        <output id="sendClickedItem"
               type="de.hybris.platform.integrationservices.model.IntegrationObjectModel"/>
    </sockets>
</widget-definition>
